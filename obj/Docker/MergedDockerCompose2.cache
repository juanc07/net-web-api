networks:
  net5tutorial:
    external: true
    name: net5tutorial
services:
  catalog:
    build:
      context: D:\work\Delaware\2022\studies\net-core-studies\net-core-studies\exercise\customAPI\CustomWebAPI\Catalog\Catalog
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Catalog
      target: prepare-restore-files
    container_name: Catalog
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      MongoDBSettings:Host: mongo
      MongoDBSettings:Password: password123
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2;/root/.nuget/fallbackpackages3
    image: jclaridad07/catalog:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2 --additionalProbingPath /root/.nuget/fallbackpackages3  "/app/bin/Debug/net6.0/Catalog.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      net5tutorial: {}
    ports:
    - published: 8080
      target: 80
    - target: 80
    - published: 8081
      target: 443
    - target: 443
    tty: true
    volumes:
    - D:\work\Delaware\2022\studies\net-core-studies\net-core-studies\exercise\customAPI\CustomWebAPI\Catalog\Catalog:/app:rw
    - C:\Users\juanc\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\juanc\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\juanc\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Program Files (x86)\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages2:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages3:ro
    - C:\Users\juanc\.nuget\packages:/root/.nuget/packages:ro
    - D:\work\Delaware\2022\studies\net-core-studies\net-core-studies\exercise\customAPI\CustomWebAPI\Catalog\Catalog:/src:rw
  mongo:
    environment:
      MONGO_INITDB_ROOT_PASSWORD: password123
      MONGO_INITDB_ROOT_USERNAME: mongoadmin
    image: mongo
    networks:
      net5tutorial: {}
    ports:
    - published: 27017
      target: 27017
    restart: always
    volumes:
    - mongodbdata:/data/db:rw
version: '3.4'
volumes:
  mongodbdata: {}