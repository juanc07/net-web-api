networks:
  net5tutorial:
    external: true
    name: net5tutorial
services:
  catalog:
    build:
      context: D:\work\Delaware\2022\studies\net-core-studies\net-core-studies\exercise\customAPI\CustomWebAPI\Catalog\Catalog
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      MongoDBSettings:Host: mongo
      MongoDBSettings:Password: password123
    image: jclaridad07/catalog:v4.1.10
    networks:
      net5tutorial: {}
    ports:
    - published: 8080
      target: 80
    - target: 80
    - published: 8081
      target: 443
    - target: 443
  mongo:
    environment:
      MONGO_INITDB_ROOT_PASSWORD: password123
      MONGO_INITDB_ROOT_USERNAME: mongoadmin
    image: mongo
    networks:
      net5tutorial: {}
    ports:
    - published: 27017
      target: 27017
    restart: always
    volumes:
    - mongodbdata:/data/db:rw
version: '3.4'
volumes:
  mongodbdata: {}